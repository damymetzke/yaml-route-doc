    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/marked": "^1.2.1",
    "@types/prompts": "^2.0.9",
    "@types/sass": "^1.16.0",
    "argparse": "^2.0.1",
    "handlebars": "^4.7.6",
    "marked": "^1.2.7",
    "prompts": "^2.4.0",
    "sass": "^1.32.0",
    "yaml": "^1.10.0"
  }
