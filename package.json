  "keywords": [
    "routing",
    "documentation",
    "yaml",
    "automation"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "yamlRouteDoc": "./lib/yamlRouteDoc.js"
  },
  "scripts": {
